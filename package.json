{
  "name": "kroger-component-seed",
  "version": "0.1.0",
  "description": "Kroger React Component Seed",
  "scripts": {
    "start": "harness",
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "ava",
    "watch": "ava --poll"
  },
  "author": "Kroger",
  "license": "UNLICENSED",
  "dependencies": {
    "axios": "0.9.1",
    "babel-polyfill": "6.7.4",
    "nock": "7.7.2",
    "react": "0.14.7",
    "react-dom": "0.14.7",
    "react-redux": "4.4.1",
    "redux": "3.3.1",
    "redux-thunk": "2.0.1",
    "sinon": "1.17.3"
  },
  "devDependencies": {
    "ava": "0.13.0-kroger",
    "enzyme": "2.2.0",
    "eslint": "2.5.3",
    "eslint-config-kroger": "0.2.1",
    "kroger-component-dev-harness": "0.2.1"
  },
  "ava": {
    "files": [
      "src/**/*.spec.js"
    ],
    "source": [
      "src/**/*.js"
    ],
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "react"
    ]
  }
}
